{
    "name": "deantev/inhealth-integration",
    "description": "Mandiri Inhealth integration api version: v2",
    "keywords": ["mandiri", "asuransi", "insurance", "inhealth"],
    "authors": [
        {
            "name": "Tevin Dean Ramadhan",
            "email": "deantevinn.work@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/deantevin/inhealth-integration"
        }
    ],
    "autoload": {
        "psr-4": {
            "Deantev\\Inhealth\\Integration\\": "src"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.5.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },    
    "extra": {
        "laravel": {
            "providers": [
                "Deantev\\Inhealth\\Integration\\InhealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}